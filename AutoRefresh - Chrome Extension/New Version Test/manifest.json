    
{
	"manifest_version": 2,
 	"name": "Auto Refresh for Jira [By Gustavo Pimenta]",
 	"description": "Extensao para Atualizar ServiceDesk do Jira",
 	"version": "1.0",
 	"permissions": [
 		"http://*/*",
 		"https://*/*",
 		"tabs"
 	],
 	"browser_action": {
  		"default_icon" : "icon.png",
  		"default_popup": "popup.html"
	},
 	"icons": { 
  		"128" : "icon_128.png"
	},
	"background": {
		"scripts": ["app.js"]
	}
}